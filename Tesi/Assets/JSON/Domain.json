						[ "andEffects", 
							[ "at_start", [ "false", [ "predicate", "free", ["array", "r"]]]],
							[ "at_end", [ "true", [ "predicate", "free", ["array", "r"]]]],
							[ "at_end", [ "decrease", ["function", "battery-amount", ["array", "r"]], "2"]],
							[ "at_end", [ "increase", ["function", "posY", ["array", "r"]], "1"]]
						],
						[ "kronosim_period", 120],
						[ "kronosim_computation_cost", 40]
					]
				]
